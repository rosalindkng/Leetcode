class Solution:
    def romanToInt(self, s: str) -> int:
        dict = {'I':1, 'V':5, 'X': 10, 'L':50, 'C': 100, 'D': 500, 'M':1000}
        
        total = 0
        for i, str in enumerate(s):
            if((i < len(s)-1) and (dict[str] < dict[s[i+1]])):
                total -= dict[str]
            else:
                total += dict[str]
        return total
